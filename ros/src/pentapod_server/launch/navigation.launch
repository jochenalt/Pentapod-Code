<!--
  navigation stack
 -->
<launch>
   <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
      <rosparam file="$(find pentapod_server)/costmaps/costmap_common_params.yaml" command="load" ns="global_costmap" />
      <rosparam file="$(find pentapod_server)/costmaps/local_costmap_params.yaml" command="load" ns="local_costmap" /> 
      <rosparam file="$(find pentapod_server)/costmaps/global_costmap_params.yaml" command="load" />
      <rosparam file="$(find pentapod_server)/costmaps/teb_local_planner_params.yaml" command="load" />  
      <param name="base_local_planner" value="teb_local_planner/TebLocalPlannerROS" />
      <param name="controller_frequency" value="5.0" />
      <param name="controller_patience" value="10.0" />
      <param name="clearing_rotation_allowed" value="false" />
   </node>
</launch>
